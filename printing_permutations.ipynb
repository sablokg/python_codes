   "source": [
    "# print only odd first indexed permutations\n",
    "from itertools import permutations\n",
    "data = [1, 2, 3]\n",
    "for i in list(permutations(data)):\n",
    "    if i[0] == 2:\n",
    "        continue\n",
    "    print(i, sum(i))"
   ]
  }
