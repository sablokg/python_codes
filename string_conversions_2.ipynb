   "source": [
    "# Convert a list of characters in a string\n",
    "\n",
    "s=input('\\nEnter a string : ') \n",
    "\n",
    "x=set(s)\n",
    "print()\n",
    "print('Using set()               :',x)\n",
    "\n",
    "x=set()\n",
    "for c in s:\n",
    " x.add(c)\n",
    "\n",
    "print()\n",
    "print('Using for..in loop        :',x)\n",
    "\n",
    "x={c for c in s}\n",
    "\n",
    "print()\n",
    "print('Using Set Comprehension   :',x)"
   ]
  }
