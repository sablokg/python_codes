    "# To filter list based on even and odd numbers\n",
    "l=[25,40,10,20,10,25,35,40,25,15,10,40,55,10,20,33,15,10]\n",
    "list_count_odd=[]\n",
    "list_count_even=[]\n",
    "for n in l:\n",
    "    if n%2==0:\n",
    "        list_count_even.append(n)\n",
    "        continue\n",
    "    if n%2==1:\n",
    "        list_count_odd.append(n)"
   ]
